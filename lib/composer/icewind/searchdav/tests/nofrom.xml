<?xml version="1.0"?>
<d:searchrequest xmlns:d="DAV:">
	<d:basicsearch>
		<d:select>
			<d:prop>
				<d:getcontentlength/>
			</d:prop>
		</d:select>
		<d:where>
			<d:gt>
				<d:prop>
					<d:getcontentlength/>
				</d:prop>
				<d:literal>10000</d:literal>
			</d:gt>
		</d:where>
		<d:orderby>
			<d:order>
				<d:prop>
					<d:getcontentlength/>
				</d:prop>
				<d:ascending/>
			</d:order>
		</d:orderby>
	</d:basicsearch>
</d:searchrequest>
